<%= form_for @game do |f| %>
<%= f.label :game_time %>
<%= f.time_field :game_time %>
<%= f.label :game_date %>
<%= f.date_field :game_date %>
<%= f.label :location %>
<%= f.text_field :location %>
<%= f.label :team_id %>
<%= f.collection_select :team_id, Team.all, :id, :name %>

<%= f.submit %>
<% end %>
